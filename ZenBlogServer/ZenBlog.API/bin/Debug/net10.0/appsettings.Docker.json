{
  "ConnectionStrings": {
    "ZenBlogConnection": "Host=postgres;Port=5432;Database=zenblog;Username=postgres;Password=postgres"
  },
  "HttpsRedirection": {
    "Enabled": false
  },
  "Redis": {
    "Enabled": true,
    "ConnectionString": "redis:6379"
  },
  "RabbitMq": {
    "Enabled": true,
    "Host": "rabbitmq",
    "Port": 5672,
    "VirtualHost": "/",
    "Username": "zenblog",
    "Password": "zenblog"
  },
  "Storage": {
    "Provider": "S3"
  },
  "AwsS3": {
    "Enabled": true,
    "Region": "us-east-1",
    "BucketName": "zenblog-local",
    "KeyPrefix": "uploads/",
    "MakePublic": true,
    "ServiceUrl": "http://localstack:4566",
    "ForcePathStyle": true,
    "PublicBaseUrl": "http://localhost:4566/zenblog-local"
  },
  "OpenApi": {
    "Enabled": true
  },
  "Database": {
    "MigrateOnStartup": true
  },
  "EmailParameters": {
    "Host": "smtp.gmail.com",
    "Port": 587,
    "EnableSsl": true,
    "From": "",
    "Username": "",
    "AppPassword": ""
  },
  "DataProtection": {
    "KeysPath": "/app/keys"
  }
}